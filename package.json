{
  "name": "fellowary-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "chokidar-nonbundled": "yarn chokidar -c \"./chokidar_copy.sh {event} {path}\" src/nonbundled/*",
    "setup": "cp node_modules/webextension-polyfill/dist/browser-polyfill.js public/js/browser-polyfill.js",
    "ext": "yarn web-ext run -t chromium -s ./build",
    "start": "concurrently \"./nonbundled_start.sh src/nonbundled/js/ public/js/ && yarn chokidar-nonbundled\"  \"react-app-rewired start\"",
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build && cp -r ./build/ ./deploy && ./nonbundled_build.sh src/nonbundled/js/ deploy/js/ && node sri.js",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,less}\"",
    "eslint": "eslint --ext .js,jsx src",
    "stylelint": "stylelint \"src/**/*.{scss,less}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"npm run lint-staged\""
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
